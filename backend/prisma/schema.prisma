generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Barbershop {
  id        Int       @id @default(autoincrement())
  name      String
  address   String
  img       String
  phone     String
  latitude  Float
  longitude Float
  createdAt DateTime? @default(now())
  updatedAt DateTime? @updatedAt()
}
